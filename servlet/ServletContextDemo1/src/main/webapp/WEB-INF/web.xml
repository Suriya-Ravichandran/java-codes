<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ServletContextDemo1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <context-param>
  		<param-name>movie</param-name>
  		<param-value>KGF</param-value>
  </context-param>
  <context-param>
  		<param-name>hero</param-name>
  		<param-value>Yash</param-value>
  </context-param>
  <context-param>
  		<param-name>heroine</param-name>
  		<param-value>ShrinidhiShetty</param-value>
  </context-param>
  <servlet>
     <servlet-name>ContextDemo</servlet-name>
     <servlet-class>com.jspider.webapp.ContextDemo</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>ContextDemo</servlet-name>
  		<url-pattern>/context</url-pattern>
  </servlet-mapping>
</web-app>